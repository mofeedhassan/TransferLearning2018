   <SOURCE>
      <ID>bluk-bnb</ID>
      <ENDPOINT>http://bnb.data.bl.uk/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?a a foaf:Person ; bibliographic:has_created ?work .</RESTRICTION>
      <PROPERTY>bibliographic:has_created/dcterms:title</PROPERTY>
      <PROPERTY>foaf:name</PROPERTY>
      <PROPERTY>bibliographic:hasCreated/bibo:isbn10</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>dbpedia</ID>
      <ENDPOINT>http://dbpedia.org/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?novel dbpediaowl:author ?b .</RESTRICTION>
      <PROPERTY>dbpediaowl:author/dbpp:name[@lang='en']</PROPERTY>
      <PROPERTY>foaf:name</PROPERTY>
      <PROPERTY>dbpediaowl:author/dbpp:isbn</PROPERTY>
   </TARGET>
   <METRIC>ADD(0.333333333333333333*Equality(a.bibliographic:has_created.dcterms:title, b\dbpediaowl:author.dbpp:name[@lang='en']), 0.333333333333333333*trigrams(a.foaf:name, b.foaf:name), 0.333333333333333333*Equality(a.bibliographic:hasCreated.bibo:isbn10, b\dbpediaowl:author.dbpp:isbn))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.60</THRESHOLD>
      <FILE>bnb-dbpedia-novelists.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.60</THRESHOLD>
      <FILE>bnb-dbpedia-novelists.nt</FILE>
      <RELATION/>
   </REVIEW>
   <EXECUTION>Simple</EXECUTION>
   <OUTPUT>TAB</OUTPUT>
</LIMES>
