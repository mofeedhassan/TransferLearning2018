      <LABEL>diseasome</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www4.wiwiss.fu-berlin.de/sider/resource/sider/</NAMESPACE>
      <LABEL>sider</LABEL>
   </PREFIX>
   <SOURCE>
      <ID>dbpedia</ID>
      <ENDPOINT>http://dbpedia.org/sparql</ENDPOINT>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?a rdf:type dbpedia:Disease</RESTRICTION>
      <PROPERTY>foaf:name AS lowercase</PROPERTY>
      <PROPERTY>rdfs:label AS lowercase</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>sider</ID>
      <ENDPOINT>http://www4.wiwiss.fu-berlin.de/sider/sparql</ENDPOINT>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?b rdf:type sider:side_effects</RESTRICTION>
      <PROPERTY>rdfs:label AS lowercase</PROPERTY>
      <PROPERTY>sider:sideEffectName AS lowercase</PROPERTY>
   </TARGET>
   <METRIC>MAX(levenshtein(a.foaf:name, b.rdfs:label), MAX(levenshtein(a.rdfs:label, b.rdfs:label), MAX(levenshtein(a.foaf:name, b.sider:sideEffectName), levenshtein(a.rdfs:label, b.sider:sideEffectName))))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.33</THRESHOLD>
      <FILE>accept.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.33</THRESHOLD>
      <FILE>review.nt</FILE>
      <RELATION/>
   </REVIEW>
   <EXECUTION>Simple</EXECUTION>
   <OUTPUT>NT</OUTPUT>
</LIMES>
