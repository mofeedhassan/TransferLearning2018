<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE LIMES
  SYSTEM "limes.dtd">
<LIMES>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
      <LABEL>rdfs</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
      <LABEL>dbpedia</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
      <LABEL>dbpedia-owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://umbel.org/umbel/sc/</NAMESPACE>
      <LABEL>umbel-sc</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://linkedgeodata.org/triplify/</NAMESPACE>
      <LABEL>linkedgeodata</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://linkedgeodata.org/property/</NAMESPACE>
      <LABEL>lgdp</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://linkedgeodata.org/ontology/</NAMESPACE>
      <LABEL>lgdo</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
      <LABEL>owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2003/01/geo/wgs84_pos#</NAMESPACE>
      <LABEL>geo</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://xmlns.com/foaf/0.1/</NAMESPACE>
      <LABEL>foaf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2003/01/geo/wgs84_pos#</NAMESPACE>
      <LABEL>wgs84</LABEL>
   </PREFIX>
   <SOURCE>
      <ID>dbpedia</ID>
      <ENDPOINT>http://live.dbpedia.org/sparql/</ENDPOINT>
      <GRAPH/>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?a a dbpedia-owl:Country. ?a wgs84:lat ?lat. ?a wgs84:long ?long.</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>wgs84:lat</PROPERTY>
      <PROPERTY>wgs84:long</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>linkedgeodata</ID>
      <ENDPOINT>http://linkedgeodata.org/sparql/</ENDPOINT>
      <GRAPH/>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?b a lgdo:Country. ?b wgs84:lat ?lat. ?b wgs84:long ?long.</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>wgs84:lat</PROPERTY>
      <PROPERTY>wgs84:long</PROPERTY>
   </TARGET>
   <METRIC>ADD(0.5*MAX(Jaro(a.rdfs:label, b.rdfs:label)), 0.5*WGS84(a.wgs84:lat, b.wgs84:lat))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.95</THRESHOLD>
      <FILE>1301310200652/country/links_accepted.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.95</THRESHOLD>
      <FILE>1301310200652/country/links_accepted.nt</FILE>
      <RELATION/>
   </REVIEW>
   <EXECUTION>Simple</EXECUTION>
   <OUTPUT>NT</OUTPUT>
</LIMES>
