<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE LIMES
  SYSTEM "limes.dtd">
<LIMES>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
      <LABEL>rdf</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
      <LABEL>rdfs</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
      <LABEL>owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
      <LABEL>dbpedia-owl</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://dbpedia.org/property/</NAMESPACE>
      <LABEL>dbpedia-prop</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://purl.org/dc/terms/</NAMESPACE>
      <LABEL>dc</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://data.linkedmdb.org/resource/movie/</NAMESPACE>
      <LABEL>linkedmdb</LABEL>
   </PREFIX>
   <PREFIX>
      <NAMESPACE>http://xmlns.com/foaf/0.1/</NAMESPACE>
      <LABEL>foaf</LABEL>
   </PREFIX>
   <SOURCE>
      <ID>dbpedia</ID>
      <ENDPOINT>http://dbpedia.org/sparql</ENDPOINT>
      <GRAPH/>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?a rdf:type dbpedia-owl:Film</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>dbpedia-owl:director/rdfs:label</PROPERTY>
      <PROPERTY>dbpedia-owl:releaseDate</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>linkedmdb</ID>
      <ENDPOINT>http://data.linkedmdb.org/sparql</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?b</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>?b rdf:type linkedmdb:film</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>linkedmdb:director/rdfs:label</PROPERTY>
      <PROPERTY>linkedmdb:initial_release_date</PROPERTY>
   </TARGET>
   <METRIC>ADD(0.333333333333333333*Levenshtein(, ), 0.333333333333333333*Levenshtein(a.dbpedia-owl:director.rdfs:label, ), 0.333333333333333333*Equality(a.dbpedia-owl:releaseDate, b.linkedmdb:initial_release_date))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.98</THRESHOLD>
      <FILE>dbpedia_linkedmdb_films.nt</FILE>
      <RELATION/>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.98</THRESHOLD>
      <FILE>dbpedia_linkedmdb_films.nt</FILE>
      <RELATION/>
   </REVIEW>
   <EXECUTION>Simple</EXECUTION>
   <OUTPUT>TAB</OUTPUT>
</LIMES>
