   <SOURCE>
      <ID>dbpedia</ID>
      <ENDPOINT>http://dbpedia.org/sparql</ENDPOINT>
      <GRAPH/>
      <VAR>?a</VAR>
      <PAGESIZE>1000</PAGESIZE>
      <RESTRICTION>{?a rdf:type dbpedia-owl:City} UNION {?a rdf:type dbpedia-owl:Place}.</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>foaf:name</PROPERTY>
   </SOURCE>
   <TARGET>
      <ID>linkedct</ID>
      <ENDPOINT>/media/mofeed/TOSHIBA2T/Samsumg1T/03_Work/AllDumps/LinkedCT/linkedct-city.n3</ENDPOINT>
      <GRAPH>0</GRAPH>
      <VAR>?b</VAR>
      <PAGESIZE>-1</PAGESIZE>
      <RESTRICTION>?b rdf:type linkedct:city .</RESTRICTION>
      <PROPERTY>rdfs:label</PROPERTY>
      <PROPERTY>linkedct:city_city_name</PROPERTY>
      <TYPE>NT</TYPE>
   </TARGET>
   <METRIC>MAX(Levenshtein(a.rdfs:label, b.rdfs:label), trigrams(a.foaf:name, b.foaf:city_city_name))</METRIC>
   <ACCEPTANCE>
      <THRESHOLD>0.92</THRESHOLD>
      <FILE>accept.nt</FILE>
      <RELATION>owl:sameAs</RELATION>
   </ACCEPTANCE>
   <REVIEW>
      <THRESHOLD>0.92</THRESHOLD>
      <FILE>review.nt</FILE>
      <RELATION>owl:sameAs</RELATION>
   </REVIEW>
   <EXECUTION>Simple</EXECUTION>
   <OUTPUT>NT</OUTPUT>
